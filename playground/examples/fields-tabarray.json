{
  "schema": {
    "friends": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "Friend",
        "properties": {
          "nick": {
            "type": "string",
            "title": "Nickname"
          },
          "animals": {
            "type": "array",
            "items": {
              "type": "string",
              "title": "Animal name"
            }
          }
        }
      }
    },
    "notDraggableFamily": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "Family member",
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          }
        }
      }
    }
  },
  "form": [
    {
      "key": "friends",
      "type": "tabarray",
      "items": {
        "type": "section",
        "items": [
          "friends[].nick",
          {
            "type": "array",
            "items": [
              "friends[].animals[]"
            ]
          }
        ]
      }
    },
    {
      "key": "notDraggableFamily",
      "type": "tabarray",
      "draggable": false,
      "items": {
        "type": "section",
        "items": [
          "notDraggableFamily[].name"
        ]
      }
    },
    {
      "type": "submit",
      "title": "Submit"
    }
  ]
}
